@model IEnumerable<Cine.AppWeb.CineWS.Cargo>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <link href="~/Content/tablas.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Crear")
    </p>
    <div class ="datagrid">

        <table>
            <thead>
                <tr>
                    <th>
                        Descripcion
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.idCargo)
                    </th>
                    <th></th>
                </tr>
            </thead>

            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.descripcion)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.idCargo)
                        </td>
                        <td>
                            @Html.ActionLink("Edit", "Actualizar", new {id=item.idCargo}) |
                            @Html.ActionLink("Details", "Details", new { /* id=item.PrimaryKey */ }) |
                         
                        </td>
                    </tr>
                }
            </tbody>


        </table>
    </divclass>
   
</body>
</html>
